#include<iostream>
#include<bits/stdc++.h>

using namespace std;


string reversestring(string s)
{
    string str;
    int len = s.length();
    
    for(int i=(len-1);i>=0;i--)
    {
        str = str + s[i];
    }
    
    return str;
}



int main()
{
    string s;
    cin>>s;
    
    cout<<reversestring(s);
     
     
    return 0;
}
